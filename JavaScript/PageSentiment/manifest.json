{
  "manifest_version": 2,
  "name": "Page Sentiment",
  "description": "Highlights the page paragraphs and text to indicate their emotion",
  "version": "0.9",
  "permissions": [
    "https://secure.flickr.com/"
  ],
  "browser_action": {
    "default_icon": "icons/extension_icon.png"
  },
  "background": {
    "persistent": false,
    "page": "sentiment.html"
  }
}
